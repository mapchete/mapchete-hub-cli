process: mapchete.processes.convert
input:
    inp: https://ungarj.github.io/mapchete_testdata/tiled_data/raster/cleantopo/
output:
    format: GTiff
    bands: 1
    dtype: uint16
    path: /tmp/mhub_cli_test/
pyramid:
    grid: geodetic
    metatiling: 2
zoom_levels:
    min: 0
    max: 8
bounds: [0, 1, 2, 3]
